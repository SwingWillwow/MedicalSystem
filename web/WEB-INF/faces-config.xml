<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>message</managed-bean-name>
        <managed-bean-class>com.demo.jsf.Message</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>dateBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.dateBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>captchaBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.CaptchaBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>nameBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.NameBean</managed-bean-class>
    
        <managed-bean-scope>none</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>tableTest</managed-bean-name>
        <managed-bean-class>com.jsfbean.TableTest</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>patientManagedBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.PatientManagedBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>sessionManagedBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.SessionManagedBean</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>regPerson</managed-bean-name>
        <managed-bean-class>com.jsfbean.RegPerson</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <context-param> 
   <param-name>com.sun.faces.disableUnicodeEscaping</param-name> 
   <param-value>auto</param-value> 
    </context-param>
    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>com.jsfbean.UserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
